	public static String num1(String a) {
		String number = "";
		char[] arr = a.toCharArray();
		for (int i = 0; i < 4; i++) {
			char ch = arr[i];
			number += ch;
		}
		return number;
	}

	public static String string1(String b) {
		String number = "";
		char[] arr = b.toCharArray();
		for (int i = 5; i < 9; i++) {
			char ch = arr[i];
			number += ch;
		}
		return number;
	}

	public static String string2(String c) {
		String number = "";
		char[] arr = c.toCharArray();
		for (int i = 10; i < 14; i++) {
			char ch = arr[i];
			number += ch;
		}
		return number;
	}

	public static String num2(String d) {
		String number = "";
		char[] arr = d.toCharArray();
		for (int i = 15; i < 19; i++) {
			char ch = arr[i];
			number += ch;
		}
		return number;
	}
}
