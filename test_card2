package mymy;

public class Test2 {
	public static void main(String[] args) {
		String card = "4455-QW34-F2J7-6789";
		String result1 = num1(card);
		int a = Integer.parseInt(result1);
		System.out.println(a);

		String result2 = string1(card);
		System.out.println(result2);

		String result3 = string1(card);
		System.out.println(result3);

		String result4 = num2(card);
		int b = Integer.parseInt(result4);
		System.out.println(b);

	}

